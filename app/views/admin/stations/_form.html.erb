<%= form_for [:admin, @station] do |f| %>
  <div class=''>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class=''>
    <%= f.label :dock_count %>
    <%= f.number_field :dock_count, min: 1 %>
  </div>

  <div class=''>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>

  <div class=''>
    <%= f.label :installation_date %>
    <%= f.date_field :installation_date %>
  </div>

  <div class=''>
    <%= f.submit %>
  </div>
<% end %>
