<div class="container-fixed">
  <div class="row">
    <div class="col-5-table">
      <table>
        <tr>
          <th>Station Name</th>
          <th>Dock Count</th>
          <th>City</th>
          <th>Installation Date</th>
        </tr>
        <% @stations.each do |station| %>
          <tr>
            <td><%= link_to station.name, station_path(station.slug) %></a></td>
            <td><%= station.dock_count %></td>
            <td><%= station.city %></td>
            <td><%= station.installation_date %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
